# Setup environment for ansible playbooks
#
# usage: source .ansible_env

# Required for some mac versions
# http://stackoverflow.com/questions/37690054/python-and-openssl-version-reference-issue-on-osx
export CRYPTOGRAPHY_ALLOW_OPENSSL_098=1

# Authentication for ansible network modules
export ANSIBLE_NET_USERNAME=admin
export ANSIBLE_NET_PASSWORD=cisco
